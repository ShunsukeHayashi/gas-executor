{
  "name": "gas-executor-mcp",
  "version": "1.0.0",
  "description": "MCP Server for Google Apps Script Executor - Execute JavaScript in Google Workspace",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gas-executor-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "dev:chatgpt": "tsx src/chatgpt/server.ts",
    "start": "node dist/index.js",
    "start:chatgpt": "node dist/chatgpt/server.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "google-apps-script",
    "gas",
    "google-workspace",
    "automation",
    "gmail",
    "google-drive",
    "google-sheets",
    "google-docs"
  ],
  "author": "Shunsuke Hayashi",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
